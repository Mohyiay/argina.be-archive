<script>
var RevolutionSlider = function () {
return {
//Revolution Slider - Full Width
initRSfullWidth: function () {
var revapi;
jQuery(document).ready(function() {
revapi = jQuery('.tp-banner').revolution(
{
delay:6000,
startwidth:1170,
startheight:600,
hideThumbs:10,
navigationStyle:"preview4",
dottedOverlay:"none",
shadow:0
});
});
},
};
}();
</script>
<script type="text/javascript">
jQuery(document).ready(function() {
App.init();
// App.initParallaxBg();
//FancyBox.initFancybox();
$(".fancybox").fancybox();
// OwlCarousel.initOwlCarousel();
RevolutionSlider.initRSfullWidth();
$('body').on('hidden.bs.modal', '.modal', function () {
$(this).removeData('bs.modal');
});
//frmIntervention
var allowed_file_size = "5242880"; //allowed file size (5Mb)
var allowed_files = [
'image/png',
'image/jpeg',
'application/msword',
'application/vnd.openxmlformats-officedocument.wordprocessingml.document',
'application/pdf',
'application/zip',
'multipart/x-zip'
]; //allowed file types
var border_color = "#bbbbbb"; //initial input border color
var bg_color = "#ffffff";
$("#frmIntervention").submit(function(e){
e.preventDefault(); //prevent default action
proceed = true; //set proceed flat to true
if(grecaptcha.getResponse().length === 0){
alert( "Please click on the reCAPTCHA box." );
proceed = false;
}
//simple input validation
$($(this).find("input[data-required=true], textarea[data-required=true]")).each(function(){
if(!$.trim($(this).val())){ //if this field is empty
$(this).css('background-color','rgba(255, 0, 0, 0.08)'); //change background-color to transparent
$(this).css('border-color','red'); //change border color to red
proceed = false; //set do not proceed flag
}
//check invalid email
var email_reg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
if($(this).attr("type")=="email" && !email_reg.test($.trim($(this).val()))){
$(this).css('border-color','red'); //change border color to red
$(this).css('background-color','rgba(255, 0, 0, 0.08)'); //change background-color to transparent red
proceed = false; //set do not proceed flag
}
}).on("input", function(){ //change border color to original
$(this).css('border-color', border_color);
$(this).css('background-color', bg_color);
});
//check file size and type before upload, works in modern browsers
if(window.File && window.FileReader && window.FileList && window.Blob){
var total_files_size = 0;
$(this.elements['bijlage'].files).each(function(i, ifile){
if(ifile.value !== ""){ //continue only if file(s) are selected
if(allowed_files.indexOf(ifile.type) === -1){ //check unsupported file
alert( ifile.name + " is unsupported file type!");
proceed = false;
}
total_files_size = total_files_size + ifile.size; //add file size to total size
}
});
if(total_files_size > allowed_file_size){
alert( "Make sure total file size is less than 5 MB!");
proceed = false;
}
}
//if everything's ok, continue with Ajax form submit
if(proceed){
var post_url = $(this).attr("action"); //get form action url
var request_method = $(this).attr("method"); //get form GET/POST method
//var form_data = new FormData(this); //constructs key/value pairs representing fields and values
var fd = new FormData(document.getElementById("frmIntervention"));
$.ajax({ //ajax form submit
url : post_url,
type: request_method,
data : fd,
dataType : "json",
contentType: false,
cache: false,
processData:false
}).done(function(result){ //fetch server "json" messages when done
result = JSON.parse(result);
if(result.type == "error"){
$("#frmIntervention").hide();
$("#intervention_result").html('<div class="alert alert-danger">'+ result.msg +"</div>").show();
}
if(result.type == "success"){
$("#frmIntervention").hide();
$("#intervention_result").html('<div class="alert alert-success">'+ result.msg +"</div>").show();
}
});
}
});
//contact form
$("#frmContact").submit(function(e){
e.preventDefault(); //prevent default action
proceed = true; //set proceed flat to true
//console.log(grecaptcha.getResponse());
// return;
if(grecaptcha.getResponse().length === 0){
alert( "Please click on the reCAPTCHA box." );
proceed = false;
}
//simple input validation
$($(this).find("input[data-required=true], textarea[data-required=true]")).each(function(){
if(!$.trim($(this).val())){ //if this field is empty
$(this).css('background-color','rgba(255, 0, 0, 0.08)'); //change background-color to transparent
$(this).css('border-color','red'); //change border color to red
proceed = false; //set do not proceed flag
}
//check invalid email
var email_reg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
if($(this).attr("type")=="email" && !email_reg.test($.trim($(this).val()))){
$(this).css('border-color','red'); //change border color to red
$(this).css('background-color','rgba(255, 0, 0, 0.08)'); //change background-color to transparent red
proceed = false; //set do not proceed flag
}
}).on("input", function(){ //change border color to original
$(this).css('border-color', border_color);
$(this).css('background-color', bg_color);
});
//if everything's ok, continue with Ajax form submit
if(proceed){
var post_url = $(this).attr("action"); //get form action url
var request_method = $(this).attr("method"); //get form GET/POST method
//var form_data = new FormData(this); //constructs key/value pairs representing fields and values
var fd = new FormData(document.getElementById("frmContact"));
$.ajax({ //ajax form submit
url : post_url,
type: request_method,
data : fd,
dataType : "json",
contentType: false,
cache: false,
processData:false
}).done(function(result){ //fetch server "json" messages when done
result = JSON.parse(result);
if(result.type == "error"){
$("#frmContact").hide();
$("#frmContact_result").html('<div class="alert alert-danger">'+ result.msg +"</div>").show();
}
if(result.type == "success"){
$("#frmContact").hide();
$("#frmContact_result").html('<div class="alert alert-success">'+ result.msg +"</div>").show();
}
});
}
});
});
</script>
